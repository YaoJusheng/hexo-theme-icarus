<!-- 点击特效  -->
<% if (theme.special_efficacy.clickLove){ %>
  <script src="<%- url_for('/js/click.js') %>"></script>
<% } %>

<!-- 雪花特效 -->
<% if (theme.special_efficacy.snow){ %>
  <script type="text/javascript" src="<%- url_for('js/snow.js') %>"></script>
<% } %>

<!-- 动态线条背景 -->
<% if (theme.special_efficacy.canvas_nest.enable){ %>
  <script type="text/javascript" color="<%=theme.special_efficacy.canvas_nest.color %>" opacity="<%=theme.special_efficacy.canvas_nest.opacity %>" src="<%- url_for('//cdn.bootcss.com/canvas-nest.js/1.0.0/canvas-nest.min.js') %>"
      zIndex="<%=theme.special_efficacy.canvas_nest.zIndex %>" count="<%=theme.special_efficacy.canvas_nest.count %>" >
  </script>
<% } %>

<!-- Tidio 在线联系功能 -->
<% if (theme.tidio.enable) { %>
  <script async src="//code.tidio.co/<%= theme.tidio.key %>.js"></script>
<% } %>

<!-- Daovoice 在线沟通 -->
<% if (theme.daovoice.enable) { %>
  <script>
    (function(i,s,o,g,r,a,m){i["DaoVoiceObject"]=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;a.charset="utf-8";m.parentNode.insertBefore(a,m)})(window,document,"script",('https:' == document.location.protocol ? 'https:' : 'http:') + "//widget.daovoice.io/widget/<%= theme.daovoice.app_id %>.js","daovoice")

    daovoice('init', {
      app_id: "<%= theme.daovoice.app_id %>"
    });
    daovoice('update');

  </script>
<% } %>